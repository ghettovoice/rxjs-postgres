{
  "name": "rxjs-postgres",
  "version": "0.1.0",
  "description": "RxJs adapters and decorators for node-postgres",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "compile": "rm -rf dist && babel --source-maps -d dist src",
    "test:coveralls": "cat coverage/lcov.info | coveralls ",
    "test:cover": "rm -rf coverage && istanbul cover _mocha -- --require babel-register -u tdd --check-leaks 'tests/**/*.js'",
    "test:mocha": "mocha --require babel-register -u tdd --check-leaks 'tests/**/*.js'",
    "test": "npm run test:mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghettovoice/rxjs-postgres.git"
  },
  "keywords": [
    "rxjs-postgres",
    "rxjs",
    "postgres",
    "pg"
  ],
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghettovoice/rxjs-postgres/issues"
  },
  "homepage": "https://github.com/ghettovoice/rxjs-postgres",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "es6-error": "^4.0.0",
    "pg": "^6.1.0",
    "pg-pool": "^1.5.0",
    "rxjs": "^5.0.0-rc.4",
    "stack-trace": "0.0.9"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "types"
  ],
  "typings": "types/index"
}
